class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        if len(s) != len(t):
            return False
        
        countT, countS = {}, {}

        for i in s:
            countT[i] = 1 + countT.get(i, 0)
        for j in t:
            countS[j] = 1 + countS.get(j, 0)
        
        return countT == countS
